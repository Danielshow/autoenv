{
	"name": "autoenv",
	"displayName": "autoenv",
	"description": "Automatically add keys to .env file",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"publisher": "danielshow",
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:autoenv.activateAutoEnv"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "autoenv.activateAutoEnv",
            "title": "Add Line to ENV"
		}],
		"menus": {
			"editor/context": [{
				"when": "resourceLangId == javascript",
				"command": "autoenv.activateAutoEnv",
				"group": "YourGroup@1"
		  }]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.46.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
		"mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Danielshow/autoenv.git"
	}
}
